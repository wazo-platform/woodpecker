{
  "main": "node_modules/expo/AppEntry.js",
  "name": "woodpecker",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "electron": "expo-electron start",
    "build": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null"
  },
  "build": {
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  },
  "iohook": {
    "targets": [
      "node-73",
      "electron-73"
    ],
    "platforms": [
      "win32",
      "darwin"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "dependencies": {
    "@wazo/sdk": "^0.36.1",
    "expo": "~42.0.1",
    "expo-status-bar": "~1.0.4",
    "iohook": "^0.9.3",
    "native-base": "3.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-safe-area-context": "3.2.0",
    "react-native-svg": "12.1.1",
    "react-native-web": "~0.13.12",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.16.0",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "electron": "^6.0.12",
    "electron-builder": "^22.11.7",
    "typescript": "~4.0.0"
  },
  "private": true
}
