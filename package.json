{
  "name": "woodpecker",
  "version": "1.0.0",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null"
  },
  "main": "electron/main/index.js",
  "build": {
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  },
  "iohook": {
    "targets": [
      "node-73",
      "electron-98"
    ],
    "platforms": [
      "win32",
      "darwin"
    ],
    "arches": [
      "x64",
      "ia32",
      "arm64"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@wazo/sdk": "^0.37.1",
    "expo": "~42.0.1",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.1",
    "iohook": "^0.9.3",
    "native-base": "3.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-unimodules": "0.14.10",
    "react-native-web": "~0.13.12",
    "react-native-webrtc": "^1.92.2",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.16.0",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "electron": "^15.1.1",
    "electron-builder": "^22.11.7",
    "typescript": "~4.0.0"
  },
  "private": true
}
